{	"toJSON": true,	"extraProperties": {		"version": "2",		"classes": {			"User": {				"panelColor": "#8DB3E2",				"panel": {					"isOpen": "true",					"pathVisible": true,					"position": {						"X": 25,						"Y": 20					}				}			}		},		"model": {			"scriptMode": "manual",			"workspaceLeft": 0,			"workspaceTop": 0		}	},	"dataClasses": [		{			"name": "User",			"className": "User",			"collectionName": "Users",			"scope": "public",			"attributes": [				{					"name": "ID",					"kind": "storage",					"scope": "public",					"unique": true,					"autosequence": true,					"type": "long",					"primKey": true				},				{					"name": "firstname",					"kind": "storage",					"scope": "public",					"not_null": true,					"type": "string"				},				{					"name": "lastname",					"kind": "storage",					"scope": "public",					"not_null": true,					"type": "string"				},				{					"name": "fullname",					"kind": "calculated",					"scope": "public",					"type": "string",					"scriptKind": "javascript",					"onGet": [						{							"from": "$mainModel.User.fullname.onGet",							"userDefined": true						}					],					"onSet": [						{							"from": "$mainModel.User.fullname.onSet",							"userDefined": true						}					]				},				{					"name": "username",					"kind": "storage",					"scope": "public",					"unique": true,					"not_null": true,					"type": "string"				},				{					"name": "password",					"kind": "calculated",					"scope": "public",					"type": "string",					"scriptKind": "javascript",					"onGet": [						{							"from": "$mainModel.User.password.onGet",							"userDefined": true						}					],					"onSet": [						{							"from": "$mainModel.User.password.onSet",							"userDefined": true						}					]				},				{					"name": "email",					"kind": "storage",					"scope": "public",					"unique": true,					"not_null": true,					"type": "string",					"pattern": "^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$"				},				{					"name": "key",					"kind": "storage",					"matchColumn": "sha1key",					"scope": "publicOnServer",					"type": "string"				}			],			"methods": [				{					"name": "isPasswordValid",					"applyTo": "entity",					"scope": "publicOnServer",					"from": "$mainModel.User.entityMethods.isPasswordValid",					"userDefined": true				},				{					"name": "signup",					"applyTo": "dataClass",					"scope": "public",					"from": "$mainModel.User.methods.signup",					"userDefined": true				}			]		}	],	"type": []}