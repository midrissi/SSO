{	"toJSON": true,	"extraProperties": {		"version": "2",		"classes": {			"User": {				"panelColor": "#8DB3E2",				"panel": {					"isOpen": "true",					"pathVisible": true,					"position": {						"X": 25,						"Y": 20					}				}			},			"TmpSession": {				"panelColor": "#76923C",				"panel": {					"isOpen": "true",					"pathVisible": true,					"position": {						"X": 301,						"Y": 21					}				}			}		},		"model": {			"scriptMode": "manual",			"workspaceLeft": 0,			"workspaceTop": 0		}	},	"dataClasses": [		{			"name": "TmpSession",			"className": "TmpSession",			"collectionName": "TmpSessions",			"matchTable": "Session",			"scope": "publicOnServer",			"attributes": [				{					"name": "ID",					"kind": "storage",					"scope": "public",					"type": "uuid",					"primKey": true,					"autosequence": true,					"unique": true,					"autogenerate": true				},				{					"name": "user",					"kind": "relatedEntity",					"scope": "public",					"type": "User",					"path": "User"				}			]		},		{			"name": "User",			"className": "User",			"collectionName": "Users",			"scope": "public",			"attributes": [				{					"name": "ID",					"kind": "storage",					"scope": "public",					"unique": true,					"autosequence": true,					"type": "long",					"indexKind": "btree",					"primKey": true				},				{					"name": "firstname",					"kind": "storage",					"scope": "public",					"not_null": true,					"type": "string"				},				{					"name": "lastname",					"kind": "storage",					"scope": "public",					"not_null": true,					"type": "string"				},				{					"name": "fullname",					"kind": "calculated",					"scope": "public",					"type": "string",					"scriptKind": "javascript",					"onGet": [						{							"from": "$mainModel.User.fullname.onGet",							"userDefined": true						}					],					"onSet": [						{							"from": "$mainModel.User.fullname.onSet",							"userDefined": true						}					]				},				{					"name": "username",					"kind": "storage",					"scope": "public",					"unique": true,					"not_null": true,					"type": "string",					"indexKind": "btree"				},				{					"name": "password",					"kind": "calculated",					"scope": "public",					"type": "string",					"scriptKind": "javascript",					"onGet": [						{							"from": "$mainModel.User.password.onGet",							"userDefined": true						}					],					"onSet": [						{							"from": "$mainModel.User.password.onSet",							"userDefined": true						}					]				},				{					"name": "email",					"kind": "storage",					"scope": "public",					"unique": true,					"not_null": true,					"type": "string",					"indexKind": "btree",					"pattern": "/^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/"				},				{					"name": "key",					"kind": "storage",					"matchColumn": "sha1key",					"scope": "publicOnServer",					"type": "string"				},				{					"name": "sessions",					"kind": "relatedEntities",					"scope": "publicOnServer",					"type": "TmpSessions",					"reversePath": true,					"path": "user"				}			],			"methods": [				{					"name": "isPasswordValid",					"applyTo": "entity",					"scope": "publicOnServer",					"from": "$mainModel.User.entityMethods.isPasswordValid",					"userDefined": true				},				{					"name": "signup",					"applyTo": "dataClass",					"scope": "publicOnServer",					"from": "$mainModel.User.methods.signup",					"userDefined": true				}			]		}	],	"type": []}